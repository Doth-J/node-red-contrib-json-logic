{
   "compilerOptions": {
      "strict": true,
      "target": "es2016",
      "module": "commonjs",
      "rootDir": "src",
      "outDir": "nodes",
      "resolveJsonModule": true,
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "skipLibCheck": true
   }
}